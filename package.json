{
  "name": "load-balancer",
  "version": "2.0.0",
  "description": "Optimized load balancer for Cosmos SDK RPC endpoints",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec 'npm run build && npm start' --ext ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "lint": "eslint 'src/**/*.ts'",
    "fix": "eslint 'src/**/*.ts' --fix",
    "test": "mocha --require ts-node/register 'tests/**/*.test.ts'"
  },
  "type": "module",
  "dependencies": {
    "agentkeepalive": "^4.5.0",
    "compression": "^1.7.4",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "node-schedule": "^2.1.1",
    "p-limit": "^6.0.0",
    "pg": "^8.14.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "@types/node-schedule": "^2.1.6",
    "@types/pg": "^8.11.11",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "chai": "^4.3.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "Cordt Hanson",
  "license": "MIT",
  "repository": "https://github.com/Cordtus/lazy-loadbalancer",
  "private": false
}
